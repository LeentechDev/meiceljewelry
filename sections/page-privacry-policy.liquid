<div class="about-us-banner" style="background-image: url('{{'banner_meicel.png'| asset_url }}');">
    <div class="about-us-title">
      <h1 class="text-center">{{ page.title }}</h1>
      <div class="text-center">
        {% include 'breadcrumb' %}
      </div>
    </div>
</div>
<div class="container">
    <div class="terms-container">
        {% for block in section.blocks %}
            {{block.settings.privacy_content_text}}
        {% endfor %}
    </div>
</div>
{% schema %}
{
    "name": "Privacy Policy",
    "blocks" : [
        {
            "type": "text",
            "name": "Text",
            "settings": [
                {
                    "type": "richtext",
                    "id" : "privacy_content_text",
                    "label": "Text"
                }
            ]
        }
    ],
    "presets": [
    {
      "name": "Privacy Policy"
    }
  ]
}
{% endschema %}