/*  CUSTOM STYLE */
h1 {
    font-family: Frank Ruhl Libre;
}
h2 {
    font-family: Frank Ruhl Libre;
}
h3 {
    font-family: Frank Ruhl Libre;
}
.header-v2 {
    padding-top: 0;
    .topbar{
        padding: 15px 0;
        color: #fff;
        background-color: #FC3F93;
    }
    .topbar-right{
        background-color: #0000;
    }
}
#header .searchform input {
    border-radius: 0 !important;
}
.label-cart {
    color: #fff;
    background-color: #FC3F93;
}
.slider-home-classic {
    padding-top: 0 !important;
}
.topbar-account{
    color: #fff !important;
}
.navbar-nav.menu-main {
    width: 100%;
    display: flex;
}
.menu-main {
    li {
        flex-grow: 1;
    }
}
.h-banner-right {
    margin-left: 50px;
}
.collection_categories .content_collection .collection:first-child{
    padding: 15px;
}
h5.text-animate-3.text-banner5.margin_bottom_30 {
    width: 500px;
    margin: 15px 0;
}
a.btn-slider.btn-banner5 {
    padding: 10px 30px;
    border: 1px solid #393D47;
    background-color: #393D47;
    color: #fff;
}
.slider-home-classic .text-slider {
    bottom: 40%;
    width: 100%;
    padding: 0 300px;
    right: 0;
    left: 0;
}
.line-tes {
    display: none;
}
.BG-section.Testimonal {
    background: #FAFAFA;
    padding: 60px;
}
.margin_bottom_80 {
    margin-bottom: 0;
}
.collection_categories .banner-collection .text_collection {
    bottom: 50px;
    left: 10%;
}
.h-collection-btn {
    border: 1px solid #4A4D50;
    padding: 12px 26px;
}
.btn-section {
    color: #4A4D50;
    background-color: #fff;
    border: 1px solid #FC3F93;
    border-radius: 0;
    &:hover {
        background-color: #FFFAFC;
        color: #000000;
    }
}
.banner_minimal .text-banner a {
    color: #fff !important;
    padding: 12px 26px;
    background-color: #393D47;
}
.container-fluid.margin_bottom_80.banner_modern {
    display: none;
}
.h-article-holder {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row-reverse;
    .h-article-mobile-header {
        display: none;
    }
    .h-article-items {
        width: 33.3%;
        padding: 20px;
    }
    .h-article-date {
        text-transform: uppercase;
    }
    .h-article-btn {
        width: 33.3%;
        height: 300px;
        padding: 20px;
    }
}
.footer-v4 {
    padding-top: 30px;
    padding-bottom: 0;
    .fv4-social-icon {
        background: #F3F6FA;
        margin-left: 20px;
        padding: 5px;
        &:nth-child(1) {
            margin-left: 0;
        }
    }
    li {
        
        a {
            color: #000000;
            line-height: 24px;
        }
    }
    .content-2 {
        p {
            color: #000000;
            line-height: 24px;
        }
    }
    form input {
        margin-top: 20px;
        width: 70%;
        border: 1px solid #C1C1C1;
        padding: 8px 10px;
    }
    form button {
        padding: 18px;
        background-color: #393D47;
        color: #fff;
        font-size: 15px;
        margin-left: 5px;
    }
}
.bottom-footer {
    border-top: 1px solid #DCDCDC;
    margin-top: 20px;
    padding: 15px 0;
    .bottom-holder {
        display: flex;
        flex-wrap: wrap;
        .btm-footer-left {
            width: 50%;
            text-align: left;
            a {
                color: #000000;
                
                &:hover {
                    color: #FC3F93;
                }
            }
        }
        .btm-footer-right {
            width: 50%;
            text-align: right;
            a {
                color: #000000;
                
                &:hover {
                    color: #FC3F93;
                }
            }
        }
    }
}
.topbar-center.mobile {
    display: none;
}
.slick-Testimonal {
    .slick-prev {
        border-radius: 0;
        &:hover {
            background-color: #FC3F93;
            color: #fff;
        }
    }
    .slick-next {
        border-radius: 0;
        &:hover {
            background-color: #FC3F93;
            color: #fff;
        }
    }
}
.btn-instagram {
    width: 20%;
    margin: 0 auto;
    border: 1px solid #000000;
    padding: 15px;
    margin-top: 20px;
    img {
        margin-right: 5px;
    }
}
.filter-button {
    padding: 10px 2px;
    h4 {
        &:hover {
        color: #FC3F93;
        
        }   
    }
}
.wrap-filter-sorting{
    border: 1px solid #B0B0B0;
    padding: 5px 20px;
    span {
        color: #000000;
    }
}
button#AddToCart {
    color: #fff;
}
.content_pagination {
    ul.pagination {
        li {
            a {
                border-radius: 0 !important;
            }
            a.active {
                color: #fff;
                background-color: #FC3F93;
            }
        }
    }
}
.single-product-tab.ver3 {
    width: 50%;
    margin: 60px auto;
}
input.spr-button:hover {
    background: #FC3F93;
}
.cart-bottom .cart-button .ciloe-btn {

    &:last-child {
        background: linear-gradient(254.55deg, #FF85BB -5.06%, #FC3F93 58.76%);
        border: none;
        &:hover {
            background: linear-gradient(254.55deg, #FF85BB -5.06%, #FC3F93 58.76%);
            border: none;
        }
    }
}
.cart-btn {
    border-radius: 0 !important;
    background: #fff !important;
    color: #4A4D50 !important;
    border: 1px solid #4A4D50 !important;
    &:last-child {
        background: linear-gradient(259.25deg, #FF85BB -5.06%, #FC3F93 58.76%) !important;
        box-shadow: 0px 2px 5px 0px #ff85bb;
        color: #fff !important;
        border: none !important;
    }
}
.checkout .row {
    margin-top: 0;
}
.cart-total {
    background: #F3F6FA;
    .cart-total-bottom {
        .btn-checkout {
            width: 80%;
        }
        .btn-back {
            border-radius: 0 !important;
            background: linear-gradient(246.49deg, #FF85BB -5.06%, #FC3F93 58.76%);
            box-shadow: 0px 2px 5px 0px #ff85bb;
            border: none;
        }
    }
}
#bk-cart-subtotal-price {
    color: #FC3F93;
    font-weight: bolder;
}
.shop_table {
    margin-top: 200px;
}
.shopping-cart {
    .table-cart-bottom {
        a {
            background: #393D47;
            color: #fff;
            margin-right: 300px;
            width: 50%;
        }
        input {
            background: #393D47;
            color: #fff;
            width: 40%;
            &:hover {
                background: #393D47;
                color: #fff;
            }
        }
    }
}
.single-product-tab {
    .nav-tabs {
        li.active > a {
            color: #FC3F93;
        }
    }
}
.e-quantity {
    display: none;
}
.breadcrumb {
    li.active {
        a {
            color: #7b7b7b;
        }
    }
}
.account-holder {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    .account-register {
        width: 50%;
        height: 500px;
        background: #FAFAFA;
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        align-content: center;
        text-align: center;
        .account-btn-submit {
            width: 400px;
            padding: 10px 15px;
            background: #393D47;
            border: 1px solid #393D47;
            color: #fff;
        }
    }
    .account-login {
        width: 50%;
        height: 500px;
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        align-content: center;
        text-align: center;
        .account-btn-submit {
            width: 400px;
            padding: 10px 15px;
            background: #393D47;
            border: 1px solid #393D47;
            color: #fff;
        }
    }
}
.topbar.topbar-v7 {
    font-family: Libre Franklin;
}
.cart-list {
    .cart-title {
        font-size: 16px;
        text-transform: none;
        font-weight: 800;
    }
    .product-price {
        float: left;
    }
}
.cart-list .list .item-cart .product-details .inner-left .product-price {
        color: #000000;
}
span.minicart-number-items.enj-cartcount {
    color: #FC3F93;
}
.about-us-banner {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    width:100%;
    height:55vh;
    background-size: cover;
    background-position: center center;
    background-repeat: no-repeat;
}
.about-us-content {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    .about-us-holder{
        width: 50%;
        display: flex;
        justify-content: center;
        align-content: center;
        .about-us-center {
            width: 80%;
            height: 100%;
            h2 {
                line-height: 42px;
            }
            p {
                line-height: 26px;
            }
            ul li {
                margin: 30px 0;
                &:before {
                    content: "\2022";
                    color: #FC3F93;
                    font-weight: 600;
                    font-size: 16px;
                    line-height: 26px;
                    display: inline-block; 
                    width: 1em;
                    margin-left: -1em;
                  }
            }
        }
    }
}
.about-us-choose {
    background: #FAFAFA;
    .about-choose-title {
        text-align: center;
    }
}
.icon {
    display: none;
}
li.fs-18 {
    font-weight: 700;
}
.contact-text-holder {
    display: flex;
    .contact-img-holder {
        margin-top: 10px;
        margin-right: 20px;
    }
}
.e-contact .btn-submit {
    border-radius: 0;
    color: #fff;
}
.about-us-title {
    margin-bottom: 100px;
}
.blog-widget.blog-widget-search form input {
    border-radius: 0 !important;
    background: #F3F6FA;
}
.blog-widget-banner.blog-widget a {
    border: 1px solid #FC3F93;
    box-sizing: border-box;
    border-radius: 6px;
    color: #FC3F93;
}
.blog-post-author {
    display: none;
}
.blog-post-item .blog-post-title {
    font-family: Libre Franklin;
    line-height: 34px;
    font-weight: 600;
}
.button-blog {
    color: #FC3F93 !important;
}
.collection-product-holder {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    .collection-product-img {
        width: 30%;
        .product-img-catalog {
            width: 100%;
            display: block;
            max-width: 100%;
            margin: auto;
        }
    }
    .collection-product-info {
        padding: 20px 10px;
    }
}
.promo-tag{
    display: flex;
    .promo-tag-title {
        padding: 5px 0;
        margin-right: 10px;
    }
    a{
        border: 1px solid #FC3F93;
        box-sizing: border-box;
        border-radius: 6px;
        color: #FC3F93;
        padding: 5px 20px;
        margin: 0 10px 10px 0;
        display: inline-block;
    }
}
.blog-comment .blog-comment-bottom .btn-submit {
    background: linear-gradient(256.68deg, #FF85BB -5.06%, #FC3F93 58.76%);
    box-shadow: 4px 4px 20px rgba(233, 135, 152, 0.6);
    color: #fff;
    border-radius: 0;
    &:hover {
        opacity: 0.8;
        background: linear-gradient(256.68deg, #FF85BB -5.06%, #FC3F93 58.76%);
        box-shadow: 4px 4px 20px rgba(233, 135, 152, 0.6);
    }
}
.btn-lostpwd.spc {
    text-decoration: none;
    &:hover {
        color: #777;
    }
}
.form-error, .errors {
    width: 400px;
    margin: 20px auto;
}
@media screen and (max-width:1400px){
    .slider-home-classic .text-slider {
        padding: 0 100px;
    }
}
@media (max-width: 1299px) and (min-width: 1025px) {
    .header-v2 .topbar-right {
        display: block;
    }
}
@media screen and (max-width:992px){
    .topbar-center.mobile {
        display: block;
    }
    .slider-home-classic .text-slider {
        bottom: 20%;
        padding: 0px 30px;
    }
    .slick-Testimonal .content-comment {
        padding: 0 !important;
    }
    h5.text-animate-3.text-banner5.margin_bottom_30 {
        display: none;
    }
    .h-banner-right {
        margin-left: 0px;
    }
    .h-cl-mb {
        margin: 20px 0;
    }
    .container-fluid.margin_bottom_80.banner_modern {
        display: none;
    }
    .banner_minimal.relative {
        display: block;
    }
    .banner_modern .img_banner .absolute .btn_banner {
        padding: 15px 30px;
        font-size: 10px;
    }
    .BG-section.Testimonal {
        padding: 30px 10px;
    }
    .h-article-holder {
        flex-direction: column;
        .h-article-mobile-header {
            display: block;
            font-size: 15px;
        }
        .h-article-items {
            width: 100%;
            padding: 0;
        }
        .h-article-btn {
            width: 100%;
            height: 150px;
            h1 {
                display: none;
            }
        }
        .h-article-promo {
            text-align: center;
        }
    }
    .footer-v4 {

        .content-2 {
            p {
                text-align: left;
            }
            h1 {
                text-align: left;
            }
        }
        form input {
            margin-top: 30px;
            padding: 5px 15px;
            width: 60%;
        }
        form button {
            padding: 15px;
            width: 36%;
        }
    }
    .btm-footer-left {
        width: 100% !important;
        text-align: center !important;
    }
    .btm-footer-right {
        width: 100% !important;
        text-align: center !important;
    }
}
@media screen and (max-width:425px){
    .container-fluid.margin_bottom_80.banner_modern {
        display: block;
    }
    .banner_minimal.relative {
        display: none;
    }
    .footer-v4 {

        form button {
            width: 33%;
            margin-left: 0;
        }
    }
    .wrap-filter-sorting {
        border: 0;
        padding: 0; 
    }
    .title_shop {
        display: none;
    }
    .single-product-tab.ver3 {
        width: 100%;
    }
    .shop_table {
        margin-top: 0;
    }
    .shopping-cart {
        .table-cart-bottom {
            a {
                width: 100%;
                margin-right: 0;
            }
            input {
                width: 100%;
            }
        }
    }
    .topright-text {
        display: none !important;
    }
    .btn-instagram {
        width: 70%;
        margin-top: 20px;
    }
    .cart-btn {
        width: 100%;
        margin-bottom: 20px;
    }
    .about-us-content {
        flex-direction: column;
        .about-us-holder {
            width: 100%;
            margin-bottom: 20px;
        }
    }
    img.img-responsive.ciloe_img_fit {
        width: 100%;
    }
    .collection-product-holder {
        .collection-product-img {
            width: 100%;
        }
    }
}